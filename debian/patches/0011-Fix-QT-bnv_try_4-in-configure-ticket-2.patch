From: Alban Peignier <alban@tryphon.eu>
Date: Thu, 17 May 2012 16:58:32 +0200
Subject: Fix QT bnv_try_4 in configure - ticket 2

---
 acinclude.m4 |    2 +-
 configure    |    2 +-
 2 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/acinclude.m4 b/acinclude.m4
index 685cacd..bc6e0ed 100644
--- a/acinclude.m4
+++ b/acinclude.m4
@@ -297,7 +297,7 @@ EOF
             echo "configure: could not compile:" >&AC_FD_CC
             cat bnv_qt_main.$ac_ext >&AC_FD_CC
           else
-            bnv_try_4="$CXX $QT_LIBS $LIBS -o bnv_qt_main bnv_qt_main.o moc_bnv_qt_test.o >/dev/null 2>/dev/null"
+            bnv_try_4="$CXX -o bnv_qt_main bnv_qt_main.o moc_bnv_qt_test.o $QT_LIBS $LIBS >/dev/null 2>/dev/null"
             AC_TRY_EVAL(bnv_try_4)
             if test x"$ac_status" != x0; then
               echo "$bnv_err_4" >&AC_FD_CC
diff --git a/configure b/configure
index b099b1c..166e299 100755
--- a/configure
+++ b/configure
@@ -17409,7 +17409,7 @@ EOF
             echo "configure: could not compile:" >&5
             cat bnv_qt_main.$ac_ext >&5
           else
-            bnv_try_4="$CXX $QT_LIBS $LIBS -o bnv_qt_main bnv_qt_main.o moc_bnv_qt_test.o >/dev/null 2>/dev/null"
+            bnv_try_4="$CXX -o bnv_qt_main bnv_qt_main.o moc_bnv_qt_test.o $QT_LIBS $LIBS >/dev/null 2>/dev/null"
             { (eval echo "$as_me:$LINENO: \"$bnv_try_4\"") >&5
   (eval $bnv_try_4) 2>&5
   ac_status=$?
